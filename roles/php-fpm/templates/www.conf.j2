; generated by ansible
[{{ app }} ]
user = {{ app }}
group = {{ app }}
listen = /run/php-fpm/{{ app }}.sock
listen.owner = {{ app }}
listen.group = {{ app }}
pm = ondemand
pm.process_idle_timeout = 10s
pm.max_children = {{ phpfpm_max_children }}
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 3
